							SetWeld(wRSho,0,x,ASpeed,e,f,v3( 1.2,0.5,-0.25)*Scale,v3(105,0,-30),1) 
							SetWeld(wRArm,0,x,ASpeed,g,h,g,v3(-10,0,0),1) 
