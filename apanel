		weld2.Part0 = p.Torso
		local Smoke = Instance.new("Part",p.Torso)
		Smoke.Anchored = true
		Smoke.CanCollide = false
		Smoke.Size = Vector3.new(1,1,1)
		Smoke.CustomPhysicalProperties = PhysicalProperties.new(0,0,0,0,0)
		weld2.Part1 = Smoke
