						SetWeld(wLSho,0,x,ASpeed2,a,b,Oa,v3(180,0,0),1) 
						SetWeld(wRSho,0,x,ASpeed2,e,f,Oe,v3(180,0,0),1) 
						SetWeld(wTorso,0,x,ASpeed2,nov3,nov3,v3(0,3,0)*Scale,nov3,1) 
						wait()
					end
