							SetWeld(wLLeg,0,x,ASpeed,k,l,k,v3(-45,0,20),1) 
							SetWeld(wRThi,0,x,ASpeed,m,n,m,v3(0,0,0),1) 
							SetWeld(wRLeg,0,x,ASpeed,o,p,o,v3(0,0,0),1) 
