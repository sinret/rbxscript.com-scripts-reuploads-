		local Particle = Instance.new("ParticleEmitter",Smoke)
		Particle.Rate = 50;
		Particle.Speed = NumberRange.new(30,60);
		Particle.VelocitySpread = 4;
		Particle.Texture = "rbxassetid://133619974"
	
