	BackgroundTransparency = 0
})

for i, v in pairs(Metadata.Keyframes) do
	wait(v[1]);
	updatePercentage(v[2])
end
updatePercentage(100)

tweenObject(Top, 0.5, {
	TextTransparency = 1
})
