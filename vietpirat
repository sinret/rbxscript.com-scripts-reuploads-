local Library = Update:Window(" Viet Pirates X",Enum.KeyCode.RightControl);
 Main3 = Library:Tab("Main")
 Main = Library:Tab("Fruit")
 
 
Main:Button("Legendary Fruit",function()
local args = {
    [1] = "Legendary"
}

game:GetService("Players").LocalPlayer.PlayerGui.SpinGui.LOL.Spin.Spin.Reward:FireServer(unpack(args))
    end)
    
Main:Button("Epic Fruit",function()
local args = {
    [1] = "Epic"
}

game:GetService("Players").LocalPlayer.PlayerGui.SpinGui.LOL.Spin.Spin.Reward:FireServer(unpack(args))
    end)
    
Main:Button("Uncommon Fruit",function()
local args = {
    [1] = "Uncommon"
}

game:GetService("Players").LocalPlayer.PlayerGui.SpinGui.LOL.Spin.Spin.Reward:FireServer(unpack(args))
    end)

Main:Button("Common Fruit",function()
local args = {
    [1] = "Common"
}

game:GetService("Players").LocalPlayer.PlayerGui.SpinGui.LOL.Spin.Spin.Reward:FireServer(unpack(args))
    end)
    
Main3:Textbox("Inf Beli","text here",function(value)
getgenv().Amount = value

game:GetService("ReplicatedStorage").GiveMoney:FireServer(tostring(game.Players.LocalPlayer.Name), -getgenv().Amount)
    end)

Main3:Toggle("Auto Spawn Fruit",false,function(nigga)
local ss;
for i,v in next, game:GetService("Players").LocalPlayer.PlayerGui:GetChildren() do
 if v:FindFirstChild("LOL") then
     ss = v;
 end
end
repeat wait() until ss --not task.wait() cuz u dont need it to be faster

getgenv().a = nigga
while getgenv().a do task.wait()
local args = {
   [1] = "Legendary",
   [2] = math.huge
}

ss.LOL.Spin.Spin.Reward:FireServer(unpack(args))
end
end)

Main3:Toggle("Auto Drop Fruit",false,function(rac)
getgenv().wgwegw = rac
   while getgenv().wgwegw do task.wait()
       for _, tool in ipairs(game:GetService("Players").LocalPlayer.Backpack:GetChildren()) do
           if tool:IsA("Tool") then
                tool.Parent = game:GetService("Players").LocalPlayer.Character
           end
       end
       for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
           if v:IsA("Tool") then
           v.Parent = game.Workspace
           end
       end
   end
end)

Main3:Button("Discord",function()
setclipboard("https://dsc.gg/grimcity")
    end)
