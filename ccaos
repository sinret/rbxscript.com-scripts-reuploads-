A1:NewButton("kill", "ping must be 50 - 400", function()
    for i,v in pairs(game.Teams.Creepers:GetPlayers()) do
        if v.Name == game.Players.LocalPlayer.Name then -- checks if you are in the creepers team
            for i,v in pairs(game:GetService("CoreGui").RobloxGui.PerformanceStats:GetDescendants()) do
                if v.ClassName == "TextLabel" and v.Text == "Ping" then
                    for i,v in pairs(v.Parent:GetChildren()) do
                        if v.Name == "ValueLabel" then
                            local oldping = string.gsub(v.Text," ms","")
                            local ping = (oldping/1000) -- divides ping by 1000
                            local explodetime = 2.65 -- normal explode time - 0.05
                            local total = (explodetime-ping)
                            game:GetService("ReplicatedStorage").Remotes.Explode:InvokeServer()
                            wait(total)
                            for i,v in pairs(game.Teams.Humans:GetPlayers()) do
                                if v.Name ~= "." then -- if you dont wanna kill your friend, replace "." with your friends name
                                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.Character.HumanoidRootPart.CFrame
                                end
                            end
                        end
                    end
                end
            end
        end
    end
    for i,v in pairs(game.Teams.Humans:GetPlayers()) do
        if v.Name == game.Players.LocalPlayer.Name then -- checks if you are in the humans team
            print("bruh")
        end
    end
end)

