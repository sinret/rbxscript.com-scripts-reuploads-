A1:NewButton("show/size iron golem hitbox", "big hitbox!!", function()
    for i,v in pairs(game:GetService("Workspace").Items:GetChildren()) do
        if v.Name == "Iron Golem" then
            v.HitBox.Transparency = 0.5
            v.HitBox.CanCollide = false
            v.HitBox.Size = Vector3.new("50","50","50")
        end
    end
end)

A1:NewButton("size weapon 420!!", "bigg hitbox!!", function()
    for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
        if v.ClassName == "Tool" and v.Name ~= "Bow" and v.Name ~= "Telekinesis Gun" and v.Name ~= "Build" and v.Name ~= "Delete" and v.Name ~= "Shield" and v.Name ~= "TNT" and v.Name ~= "Golem" and v.Name ~= "Tele Pearl" and v.Name ~= "BloxyCola" and v.Name ~= "Warhammer" then
            v.Handle.CanCollide = false
            v.Handle.Size = Vector3.new("420","420","420")
        end
        if v.ClassName == "Tool" and v.Name == "Warhammer" then
            v.HitBox.Transparency = 0
            v.HitBox.Size = Vector3.new("420","420","420")
        end
        if v.ClassName == "Tool" and v.Name == "Emerald Axe" then
            v.HitBox.Transparency = 0
            v.HitBox.Size = Vector3.new("420","420","420")
        end
    end
    for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
        if v.ClassName == "Tool" and v.Name ~= "Bow" and v.Name ~= "Telekinesis Gun" and v.Name ~= "Build" and v.Name ~= "Delete" and v.Name ~= "Shield" and v.Name ~= "TNT" and v.Name ~= "Golem" and v.Name ~= "Tele Pearl" and v.Name ~= "BloxyCola" and v.Name ~= "Warhammer" then
            v.Handle.CanCollide = false
            v.Handle.Size = Vector3.new("420","420","420")
        end
        if v.ClassName == "Tool" and v.Name == "Warhammer" then
            v.HitBox.Transparency = 0
            v.HitBox.Size = Vector3.new("420","420","420")
        end
        if v.ClassName == "Tool" and v.Name == "Emerald Axe" then
            v.HitBox.Transparency = 0
            v.HitBox.Size = Vector3.new("420","420","420")
        end
    end
end)

