function deletecreepercon()
    spawn(function()
        while getgenv().deletecreepercon do
            for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
                if v.Name == "CreeperCons" then
                    v:Destroy()
                end
            end
            wait()
        end
    end)
end

function placeunanchored()
    spawn(function()
        while getgenv().placeunanchored do
            local x = math.floor(game.Players.LocalPlayer.Character.HumanoidRootPart.Position.X)
            local y = math.floor(game.Players.LocalPlayer.Character.HumanoidRootPart.Position.Y-4)
            local z = math.floor(game.Players.LocalPlayer.Character.HumanoidRootPart.Position.Z)
            local xround = (Round(x))
            local yround = (Round(y))
            local zround = (Round(z))
            local A_1 = Vector3.new(xround, yround, zround)
            local A_2 = Vector3.new(0, 0, 0)
            local A_3 = (selectedblock)
            local Event = game:GetService("ReplicatedStorage").Remotes.PlaceBlock
            Event:FireServer(A_1, A_2, A_3)
            wait()
        end
    end)
end

function statsgui()
    spawn(function()
        while getgenv().statsgui do
            local htext = (game:GetService("ReplicatedStorage").Values.Humans.Value)
            local ctext = (game:GetService("ReplicatedStorage").Values.Creepers.Value)
            local ttest = (game:GetService("ReplicatedStorage").Values.Timer.Value)
            humans.Text = htext
            creepers.Text = ctext
            timer.Text = ttest
            wait(.1)
        end
    end)
end


function deltnt()
    spawn(function()
        while getgenv().deltnt do
            for i,v in pairs(game:GetService("Workspace").Items:GetChildren()) do
                if v.Name == "WhitePart" then
                    for i,v in pairs(v:GetChildren()) do
                        if v.ClassName == "TouchTransmitter" then
                            v:Destroy()
                        end
                    end
                end
            end
            wait()
        end
    end)
end

function deletecreepercons2()
    for i, v in pairs(game:GetDescendants()) do
        local cval = game.Players.LocalPlayer.Character
        if v.Name == "CreeperCons" and v.Parent.Name == (cval) then
            v:Destroy()
        end
        wait()
    end
end

function bigarrows()
    spawn(function()
        while getgenv().bigarrows do
            for i,v in pairs(game.Workspace.Items:GetChildren()) do
                if v.Name == "Arrow" then
                    v.Size = Vector3.new("5","5","5")
                    v.CanCollide = false
                    v.Transparency = 0
                end
            end
            wait(.1)
        end
    end)
end

function tparrows()
    spawn(function()
        while getgenv().tparrows do
            for i,b in pairs(game.Teams.Creepers:GetPlayers()) do
                for i,v in pairs(game:GetService("Workspace").Items:GetChildren()) do
                    if v.Name == "Arrow" then
                        v.CFrame = b.Character.Head.CFrame
                    end
                end
            end
            wait()
        end
    end)
end



local function Round(r)
    local divided = r / 4
    local rounded = 4 * math.floor(divided)
    return rounded
end

function bbay()
    spawn(function()
        while getgenv().bbay do
            game.ReplicatedStorage.Values.Game.Value = false
            local x = math.floor(game.Players.LocalPlayer.Character.HumanoidRootPart.Position.X)
            local y = math.floor(game.Players.LocalPlayer.Character.HumanoidRootPart.Position.Y)
            local z = math.floor(game.Players.LocalPlayer.Character.HumanoidRootPart.Position.Z)
            local xround = (Round(x))
            local yround = (Round(y))
            local zround = (Round(z))
            local A_1 = Vector3.new(xround, yround-4, zround)
            local A_2 = Vector3.new(0, 0, 0)
            local A_3 = (selectedblock)
            local Event = game:GetService("ReplicatedStorage").Remotes.PlaceBlock
            Event:FireServer(A_1, A_2, A_3)
            yround = (Round(y))
            local A_1 = Vector3.new(xround, yround-8, zround)
            local A_2 = Vector3.new(0, 0, 0)
            local A_3 = (selectedblock)
            local Event = game:GetService("ReplicatedStorage").Remotes.PlaceBlock
            Event:FireServer(A_1, A_2, A_3)
            wait()
        end
    end)
end








game:GetService("ReplicatedStorage").Values:WaitForChild("Hint")
game:GetService("ReplicatedStorage").Values.Hint.Value = "Created By 2AreYouMental110"

game:GetService("ReplicatedStorage").Values.Hint:GetPropertyChangedSignal("Value"):connect(function()
    if game:GetService("ReplicatedStorage").Values.Hint.Value ~= "Created By 2AreYouMental110" then
        wait(2)
        game:GetService("ReplicatedStorage").Values.Hint.Value = "Created By 2AreYouMental110"
        wait(3)
        if game:GetService("ReplicatedStorage").Values.Hint.Value ~= "Created By 2AreYouMental110" then
            repeat
            game:GetService("ReplicatedStorage").Values.Hint.Value = "Created By 2AreYouMental110"
            wait(2)
            until game:GetService("ReplicatedStorage").Values.Hint.Value == "Created By 2AreYouMental110"
        end
    end
end)





for i,v in pairs(game.Players:GetPlayers()) do
    if v:GetRankInGroup(5683059) >= 7 then
        game:GetService("StarterGui"):SetCore("SendNotification", { 
        Title = "Moderator Already Ingame!",
        Text = (v.Name)
        })
        rconsoleprint("mod detected!",v.Name)
    end
    if v:GetRankInGroup(5683059) == 3 then
        game:GetService("StarterGui"):SetCore("SendNotification", { 
        Title = "Helper Already Ingame!",
        Text = (v.Name)
        })
        rconsoleprint("@@RED@@")
        rconsoleprint("Helper Already Ingame!",v.Name)
    end
end

game.Players.PlayerAdded:Connect(function(plr)
    wait(.5)
    if plr:GetRankInGroup(5683059) >= 7 then
        game:GetService("StarterGui"):SetCore("SendNotification", { 
        Title = "Moderator Joined!",
        Text = (plr.Name)
        })
        rconsoleprint("@@RED@@")
        rconsoleprint("mod detected!",plr.Name)
    end
    if plr:GetRankInGroup(5683059) == 3 then
        game:GetService("StarterGui"):SetCore("SendNotification", { 
        Title = "Helper Joined!",
        Text = (plr.Name)
        })
        rconsoleprint("@@RED@@")
        rconsoleprint("Helper detected!",plr.Name)
    end
    if plr:GetRankInGroup(5683059) <= 1 then
        game:GetService("StarterGui"):SetCore("SendNotification", { 
        Title = "normal player joined",
        Text = (plr.Name)
        })
    end
end)

game:GetService("StarterGui"):SetCore("SendNotification", { 
Title = "Successfully Loaded CC Script!",
Text = "Successfully Loaded CC Script!"
})
