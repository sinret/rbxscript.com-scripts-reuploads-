            print(v.Parent.Parent.Parent,v.Parent.Parent,v.Parent,v.Parent.HumanoidRootPart.CFrame)
        end
    end
    print("completed, if theres nothing printed no charged creepers are here")
end)

A1:NewButton("show scaffolding hitbox", "trusses are shown", function()
    for i,v in pairs(game.Workspace.Map.ExplodableBlocks:GetDescendants()) do
        if v.Name == "Block" and v.Value == "Scaffolding" then
            for i,v in pairs(v.Parent:GetDescendants()) do
                if v.Name == "Truss" then
                    v.Transparency = 0
                end
            end
        end
    end
end)

