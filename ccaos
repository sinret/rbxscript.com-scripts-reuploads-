A1:NewButton("Become Jesus", "walk on water", function()
    for i,v in pairs(game:GetService("Workspace").Map:GetChildren()) do
        if v.ClassName == "Part" and v.Name == "Water" then
            for i,v in pairs(v:GetChildren()) do
                if v.ClassName == "TouchTransmitter" then
                    v:Destroy()
                end
            end
        end
        if v.ClassName == "Script" and v.Name == "Water" then
            v:Destroy()
        end
    end
end)

