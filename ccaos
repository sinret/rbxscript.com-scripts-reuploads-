local selectedoption
local selectedblock
local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Creeper Chaos (Beta)", "Sentinel")

local A = Window:NewTab("Main")
local A1 = A:NewSection("Main")
local B = Window:NewTab("Other Scripts")
local B1 = B:NewSection("Other Scripts")



local drpdwn = A1:NewDropdown("players", "its not display name", {"placeholer", "placeholder"}, function(value1)
    selectedoption = value1
    print(value1)
end)

local bdrpdwn = A1:NewDropdown("blocks", "its not manual", {"placeholer", "placeholder"}, function(value1)
    selectedblock = value1
    print(value1)
    return selectedblock
end)


local playerdrpdwn = {

}
local blockdrpdwn = {

}

A1:NewButton("Update Dropdown (humans)", "Refreshes Dropdown", function()
    table.clear(playerdrpdwn)
    for i,v in pairs(game.Teams.Humans:GetPlayers()) do
        if v.Name ~= game.Players.LocalPlayer.Name then
            table.insert(playerdrpdwn,v.Name)
            drpdwn:Refresh(playerdrpdwn)
        end
    end
end)

A1:NewButton("Update Dropdown (creepers)", "Refreshes Dropdown", function()
    table.clear(playerdrpdwn)
    for i,v in pairs(game.Teams.Creepers:GetPlayers()) do
        if v.Name ~= game.Players.LocalPlayer.Name then
            table.insert(playerdrpdwn,v.Name)
            drpdwn:Refresh(playerdrpdwn)
        end
    end
end)

A1:NewButton("Update Dropdown (both)", "Refreshes Dropdown", function()
    table.clear(playerdrpdwn)
    for i,v in pairs(game.Players:GetPlayers()) do
        if v.Name ~= game.Players.LocalPlayer.Name then
            table.insert(playerdrpdwn,v.Name)
            drpdwn:Refresh(playerdrpdwn)
        end
    end
end)

A1:NewButton("Update Dropdown (blocks)", "Refreshes Dropdown (the other one)", function()
    table.clear(blockdrpdwn)
    for i,v in pairs(game:GetService("ReplicatedStorage").Blocks:GetChildren()) do
        if v.ClassName == "Part" then
            table.insert(blockdrpdwn,v.Name)
            bdrpdwn:Refresh(blockdrpdwn)
        end
    end
end)

A1:NewButton("select block", "selects block to build", function()
    local A_1 = (selectedblock)
    local Event = game:GetService("ReplicatedStorage").Remotes.SelectBlock
    Event:FireServer(A_1)
end)



A1:NewToggle("build blocks around you (selected)", "you have to select the block", function(bool)
    getgenv().bbay = bool
    if bool then
        bbay()
    end
end)


A1:NewButton("kill (selected)", "ping must be 50 - 400", function()
    for i,v in pairs(game.Teams.Creepers:GetPlayers()) do
        if v.Name == game.Players.LocalPlayer.Name then -- checks if you are in the creepers team
            for i,v in pairs(game:GetService("CoreGui").RobloxGui.PerformanceStats:GetDescendants()) do
                if v.ClassName == "TextLabel" and v.Text == "Ping" then
                    for i,v in pairs(v.Parent:GetChildren()) do
                        if v.Name == "ValueLabel" then
                            local oldping = string.gsub(v.Text," ms","")
                            local ping = (oldping/1000) -- divides ping by 1000
                            local explodetime = 2.65 -- normal explode time - 0.05
                            local total = (explodetime-ping)
                            game:GetService("ReplicatedStorage").Remotes.Explode:InvokeServer()
                            wait(total)
                            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players[selectedoption].Character.HumanoidRootPart.CFrame
                        end
                    end
                end
            end
        end
    end
    for i,v in pairs(game.Teams.Humans:GetPlayers()) do
        if v.Name == game.Players.LocalPlayer.Name then -- checks if you are in the humans team
            print("bruh")
        end
    end
end)



A1:NewButton("tp to (selected)", "a little bit sus", function()
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace")[selectedoption].HumanoidRootPart.CFrame
end)

A1:NewButton("delete shields", "HOW DID HE DAMAGE ME!!!", function()
    for i,v in pairs(game:GetDescendants()) do
        if v.Name == "Shield" and v.Parent.Name ~= "ItemWeapons" and v.Parent.Name ~= "Items" then
            v:Destroy()
        end
    end
end)

